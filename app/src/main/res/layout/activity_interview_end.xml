<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context=".GS.InterviewEnd">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/mnchmainpsbi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerInParent="true"
            android:text="@string/app_name"
            android:textColor="@color/white"
            android:textSize="30sp"
            android:textStyle="bold|italic" />

        <ImageView
            android:id="@+id/img_psbitest"
            android:layout_width="90dp"
            android:layout_height="60sp"
            android:layout_alignParentTop="true"
            android:src="@drawable/unwhite" />

    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5sp"
        android:fadeScrollbars="false"
        android:fillViewport="true"
        android:paddingBottom="5sp"
        android:paddingLeft="5sp"
        android:paddingRight="5sp"
        android:paddingTop="5sp"
        android:scrollbarSize="10dip">


        <LinearLayout style="@style/Q_Linerlayout_v">

            <!-- EditText study_id Starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView">

                <LinearLayout
                    android:id="@+id/ll_study_id"
                    style="@style/Q_Linerlayout_v">

                    <TextView
                        style="@style/TextView"
                        android:text="@string/txt_study_id" />

                    <com.santalu.widget.MaskEditText
                        android:id="@+id/ed_study_id"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/txt_study_id"
                        android:clickable="false"
                        android:focusable="false"
                        android:inputType="date"
                        app:met_mask="####-###-##" />

                </LinearLayout>

            </android.support.v7.widget.CardView>
            <!-- EditText study_id Ends -->

            <!-- Radio Buttons Q1311 Starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView">

                <LinearLayout
                    android:id="@+id/ll_Q1311"
                    style="@style/Q_Linerlayout_v">

                    <TextView
                        android:id="@+id/txt_Q1311"
                        style="@style/TextView"
                        android:text="@string/txt_Q1311" />

                    <RadioGroup style="@style/RadioGroup">

                        <RadioButton
                            android:id="@+id/rb_Q1311_1"
                            style="@style/RadioButton"
                            android:text="@string/rb_Q1311_1" />

                        <RadioButton
                            android:id="@+id/rb_Q1311_2"
                            style="@style/RadioButton"
                            android:text="@string/rb_Q1311_2" />

                        <RadioButton
                            android:id="@+id/rb_Q1311_3"
                            style="@style/RadioButton"
                            android:text="@string/rb_Q1311_3" />

                        <RadioButton
                            android:id="@+id/rb_Q1311_4"
                            style="@style/RadioButton"
                            android:text="@string/rb_Q1311_4" />

                        <RadioButton
                            android:id="@+id/rb_Q1311_5"
                            style="@style/RadioButton"
                            android:text="@string/rb_Q1311_5" />

                        <RadioButton
                            android:id="@+id/rb_Q1311_6"
                            style="@style/RadioButton"
                            android:text="@string/rb_Q1311_6" />

                        <RadioButton
                            android:id="@+id/rb_Q1311_7"
                            style="@style/RadioButton"
                            android:text="@string/rb_Q1311_7" />

                        <RadioButton
                            android:id="@+id/rb_Q1311_8"
                            style="@style/RadioButton"
                            android:text="@string/rb_Q1311_8" />

                        <RadioButton
                            android:id="@+id/rb_Q1311_9"
                            style="@style/RadioButton"
                            android:text="@string/rb_Q1311_9" />

                        <RadioButton
                            android:id="@+id/rb_Q1311_10"
                            style="@style/RadioButton"
                            android:text="@string/rb_Q1311_10" />

                        <RadioButton
                            android:id="@+id/rb_Q1311_11"
                            style="@style/RadioButton"
                            android:text="@string/rb_Q1311_11" />

                        <RadioButton
                            android:id="@+id/rb_Q1311_12"
                            style="@style/RadioButton"
                            android:text="@string/rb_Q1311_12" />

                    </RadioGroup>

                </LinearLayout>

            </android.support.v7.widget.CardView>
            <!-- Radio Buttons Q1311 Ends -->

            <Button
                android:id="@+id/btn_next"
                style="@style/Button_collder"
                android:layout_margin="10dp"
                android:text="Next" />

        </LinearLayout>

    </ScrollView>

</LinearLayout>