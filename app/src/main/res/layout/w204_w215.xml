<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="10dp"
    android:background="#EEEEEE"
    android:orientation="vertical">




    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:layout_marginBottom="5dp"
        android:fadeScrollbars="false"
        android:fillViewport="true"
        android:paddingLeft="5dp"
        android:paddingTop="5dp"
        android:paddingRight="5dp"
        android:paddingBottom="5dp"
        android:scrollbarSize="50dip">
        <!-- Parent layout starts -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#F1F1F1"
            android:orientation="vertical">





            <!--W201 Edit text Starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView">

                <LinearLayout
                    android:id="@+id/ll_W201"
                    style="@style/Q_Linerlayout_v">


                    <TextView
                        android:id="@+id/txt_W201"
                        style="@style/TextView"
                        android:text="@string/txt_W201" />

                    <EditText
                        android:id="@+id/ed_W201_d"
                        style="@style/EditText_number"
                        android:hint="@string/txt_W201_d_h" />

                    <EditText
                        android:id="@+id/ed_W201_m"
                        style="@style/EditText_number"
                        android:hint="@string/txt_W201_m_h" />

                    <EditText
                        android:id="@+id/ed_W201_y"
                        style="@style/EditText_number"
                        android:hint="@string/txt_W201_y_h" />

                </LinearLayout>

            </android.support.v7.widget.CardView>
            <!--W201 Edit text Ends-->

            <!--W202 Edit text Starts -->
